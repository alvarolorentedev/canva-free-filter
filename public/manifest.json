{
  "manifest_version": 3,
  "id": "canvafreefilter@alvarolorente.dev",
  "name": "Canva Free Filter",
  "description": "Chrome Extension for canva.com filter free resources",
  "version": "0.1.10",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
      {
          "matches": ["https://www.canva.com/*"],
          "js": ["js/content_script.js"]
      }
  ],
  "host_permissions": [
    "https://www.canva.com/*"
  ]
}
