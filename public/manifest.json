{
  "manifest_version": 3,
  "name": "Canva Free Filter",
  "description": "Chrome Extension for canva.com filter free resources",
  "version": "1.0",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
      {
          "matches": ["https://www.canva.com/*"],
          "js": ["js/vendor.js", "js/content_script.js"]
      }
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "host_permissions": [
    "https://www.canva.com/*"
  ]
}
